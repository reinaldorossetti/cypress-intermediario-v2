{
  "name": "cypress-intermediario-v2",
  "version": "1.0.0",
  "description": "Repositório do projeto v2 do curso de testes automatizados com Cypress (intermdiário) da Escola TAT no Udemy",
  "main": "cypress.config.js",
  "scripts": {
    "cy:run": "npx cypress run",
    "run:cover": "nyc --reporter=lcov --reporter=text npx cypress run",
    "report": "allure generate allure-report --clean && allure open allure-report",
    "al:open": "allure open allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/cypress-intermediario-v2.git"
  },
  "keywords": [
    "Cypress intermediário",
    "Talking About Testing",
    "E2E Testing",
    "code",
    "coverage"
  ],
  "compilerOptions": {
    "lib": [
      "ES2021"
    ],
    "target": "ES2021"
  },
  "author": "Walmyr Filho <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wlsf82/cypress-intermediario-v2/issues"
  },
  "homepage": "https://github.com/wlsf82/cypress-intermediario-v2#readme",
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@faker-js/faker": "^7.6.0",
    "@shelex/cypress-allure-plugin": "^2.37.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.7",
    "cypress": "^12.10.0",
    "cypress-mochawesome-reporter": "^3.3.0",
    "cypress-plugin-api": "^2.6.1",
    "cypress-promise": "^1.1.0",
    "esbuild": "^0.17.17",
    "mochawesome": "^7.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "mocha": "^10.2.0"
  },
  "nyc": {
    "report-dir": "coverage"
  }
}
